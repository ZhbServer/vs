                top: 1px;
                left: 1px;
                position: absolute;
            }
.defaultbar .jquery-jslider {
                height: 15px;
                background-color: #E6E6FA;
                border: 1px solid #A5B6C8;
                top: -6px;
                display: block;
                cursor: pointer;
                position: absolute;
            }
 .defaultbar .jquery-jslider-hover {
                background-color: #000080;
            }

