<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="ww" uri="/webwork"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="PRAGMA" content="NO-CACHE" >
</head> 
<script language="JavaScript">
function init()
{
   document.getElementById("mmcontent").innerHTML=document.getElementById("mcontent").value;
}
</script>
<body style="background-color:transparent" onLoad="init();">
			<div align="left"  style="height:<ww:property value="gd"/>px;font-size:<ww:property value="dx"/>px;color:<ww:property value="ys"/>;font-family:<ww:property value="zts"/>">
		<marquee height='<ww:property value="gd"/>px' direction='left'  scrollamount='<ww:property value="speed"/>' ><span id="mmcontent"></span></marquee>
		</div>
		<input type="hidden" id="mcontent" name="mcontent" value="<ww:property value="content"/>"/>
			
</body>
</html> 