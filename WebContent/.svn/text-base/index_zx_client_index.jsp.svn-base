<script type="text/javascript">



//录入终端
function noterminal(){
	 var ids=document.getElementsByName('qx');
     var clientname="";
	 for(var i=0;i<ids.length;i++) 
	 { 
	    if(ids[i].checked==true) 
	    { 
	    	clientname=document.getElementById('client_'+ids[i].value+"").value;
	    	ids[i].value=ids[i].value+"_"+clientname;
	    } 
	  } 
	  var isSel=false; 
	  for(var i=0;i<ids.length;i++) 
	   { 
	    if(ids[i].checked==true) 
	    { 
	    isSel=true; 
	     break; 
	    } 
	   } 
	   if(isSel==false) 
	   { 
	    layer.msg("<ww:property value='#session.local.a00409' />"); 
	    return ; 
	   }
	   document.form1.action="enterclient.vs";
	   document.form1.submit();
}
//清理未录入终端
function clear_enter_client(){
	var ids=document.getElementsByName('qx'); 
    var isSel=false; 
	  for(var i=0;i<ids.length;i++) 
	   { 
	    if(ids[i].checked==true) 
	    { 
	       isSel=true; 
	       break; 
	    } 
	   } 
	   if(isSel==false) 
	   { 
	      layer.msg("<ww:property value='#session.local.a00409' />"); 
	      return; 
	   }
	document.form1.action="clearLrTerminal.vs";
	document.form1.submit(); 
	
}
function jiankong(id){
	//window.location.href="jiankong.vs?id="+id;
	open_jiankong(id);
} 

function open_jiankong(cid){

	 window.returnValue="";

	 layer.open({
			type: 2,
	        area: ['720px', '400px'],
	        //offset: '20px',
	        title:false,
	        fix: false, //不固定
	        shift: Math.ceil(Math.random()*6), //0-6的动画形式，-1不开启
	        content: 'jiankong.vs?id='+cid
	        //maxmin: true
		});
}

/*查看终端信息*/
function open_client_info(cid){

	 

	 window.returnValue="";
	 layer.open({
			type: 2,
	        area: ['900px', '550px'],
	        offset: '20px',
	        title:false,
	        fix: false, //不固定
	        shift: Math.ceil(Math.random()*6), //0-6的动画形式，-1不开启
	        content: 'getclientinforefresh.vs?cid='+cid,
	        scrollbar: false,
	        end: function(){ 
	    	 
			              
			}
		});
 }

//发布批量监控
function send_jk(){
	
	var ids=document.getElementsByName('qx');
 
    var isSel=false; 
	for(var i=0;i<ids.length;i++) 
	{ 
	    if(ids[i].checked==true) 
	    { 
	    isSel=true; 
	     break; 
	    } 
     } 
	 if(isSel==false) 
	 { 
	    layer.msg("<ww:property value='#session.local.a00409' />"); 
	    return ; 
	 }

	  //layer 选择截图百分比
	 layer.open({
	      offset: '25%',
		  title: "<ww:property value='#session.local.b00269' />",
		  content: "<center><select id='show'  style='width:120px;'>"+
                   "<option value='0.8'><ww:property value='#session.local.b00271' /></option>"+
                   "<option value='0.6'><ww:property value='#session.local.b00272' /></option>"+
                   "<option value='0.4'><ww:property value='#session.local.b00273' /></option>"+
                   "<option value='0.2'><ww:property value='#session.local.b00274' /></option>"+
                   "</select></center>",
                   btn:["<ww:property value='#session.local.a00869' />","<ww:property value='#session.local.a00670' />"],

                   btn1: function (index) {

                	   var show=$('#show option:selected') .val();//选中的值
                	
                	   document.form1.action="sendJkBatch.vs?show="+show;

                	   document.form1.submit(); 
                   },
                   btn2: function (index) {
                	 
                   }      
		});  

	  //document.form1.action="sendJkBatch.vs";
	  //document.form1.submit(); 
	
}

//查看历史
function sel_jk_list(){

	window.location.href="jklist.vs";
	
}


/**导出日志**/
function export_log(cid){
	
	 window.open("seelog.vs?id="+cid);
	 
} 

</script>
