package com.vshow.control.interfaces;

import java.util.ArrayList;
import java.util.List;

import com.vshow.control.data.program.ProgramPublic;

public class InteractDll {

	private int dllInteractType;  //(类型  1文本  2图片,3清理)
	private int showType; //(显示类型 0局部 1全屏)
	private List<DllPublic> mDlls = new ArrayList<DllPublic>();//播放集
	
   
	public int getShowType() {
		return showType;
	}
	public void setShowType(int showType) {
		this.showType = showType;
	}
	public List<DllPublic> getmDlls() {
		return mDlls;
	}
	public void setmDlls(List<DllPublic> mDlls) {
		this.mDlls = mDlls;
	}
	public int getDllInteractType() {
		return dllInteractType;
	}
	public void setDllInteractType(int dllInteractType) {
		this.dllInteractType = dllInteractType;
	}
	

	
	
	
}
