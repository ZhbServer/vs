package com.vshow.control.data;

public class Rss extends PublicData {
	private int id;
	private String url;
	private String content;
	private String contentsub;
	private int freshtime;
	private String name;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}



	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public int getFreshtime() {
		return freshtime;
	}

	public void setFreshtime(int freshtime) {
		this.freshtime = freshtime;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getContentsub() {
		return contentsub;
	}

	public void setContentsub(String contentsub) {
		this.contentsub = contentsub;
	}
	@Override
	public String toString() {
		return id+"*"+url+"*"+freshtime+"*"+name;
		
	}

}
