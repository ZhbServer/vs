package com.vshow.control.data;

public class Tb extends PublicData {
	
	private int id;
	
	private String title;
	
	private int w;
	
	private int h;
	
	private int l;
	
	private int t;
	
	private int mType;  //0图片  1网址  -1 关闭台标
	
	private String mUrl; //播放地址
	
	private String mUrlName; //mType为0 查找素材名  显示地址名称
	
    private long mTimeout; // 0不刷新  -1 io口刷新
	
	public String getmUrlName() {
		return mUrlName;
	}

	public void setmUrlName(String mUrlName) {
		this.mUrlName = mUrlName;
	}

	
	private int uid;  //创建人

	public int getUid() {
		return uid;
	}

	public void setUid(int uid) {
		this.uid = uid;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

