package com.vshow.control.data.program;

import java.util.ArrayList;
import java.util.List;

/**
 * 
 * @author w 网页
 * 
 */
public class ProgramComplexHtml extends ProgramPublic {

	private int time;  //刷新时间
	private int gun;     //滚动(0不滚动)
	private List<ProgramHtml> mHtmlList=new ArrayList<ProgramHtml>(); //网页播放列表
	
	public int getTime() {
		return time;
	}
	public void setTime(int time) {
		this.time = time;
	}
	public int getGun() {
		return gun;
	}
	public void setGun(int gun) {
		this.gun = gun;
	}
	public List<ProgramHtml> getmHtmlList() {
		return mHtmlList;
	}
	public void setmHtmlList(List<ProgramHtml> mHtmlList) {
		this.mHtmlList = mHtmlList;
	}
	

	

}
