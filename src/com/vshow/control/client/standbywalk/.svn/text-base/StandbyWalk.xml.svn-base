<sqlMap namespace="StandbyWalk">

	<typeAlias alias="standbyWalk" type="com.vshow.control.data.StandbyWalk" />
	
	<insert id="insert_one_standbywalk" parameterClass="standbyWalk">
	  <![CDATA[ 
		insert
		into
		t_standby_walk
		(sdate,days,type,resultid)
		values
		(#sdate#,#days#,#type#,#resultid#)
	   ]]>
    </insert>
   
</sqlMap>
    